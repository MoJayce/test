page{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  background: #fff;
}
.search-content {
  background: white;
  width: 650rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: auto;
  padding-left: 20rpx;
  padding-bottom: auto;
  margin: auto;
  height: 70rpx;
  border-radius: 5px 5px;
  border: 1px solid #ccc;
}

.search-icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}

.search-input {
  width: 500rpx;
  font-size: 28rpx;
}

.search-btn {
  width: 150rpx;
  height: 70rpx;
  margin-right: 0rpx;
  background-color: #bbb;
  font-size: 28rpx;
  align-items: center;
  border-top-left-radius: 0rpx;
  border-bottom-left-radius: 0rpx;
  color: #fff;
  border: none;
  position: relative;
}

.classifys {
  display: flex;
  flex-direction: row;
  margin: 10px auto;
  box-sizing: border-box;
 height: 100%;
  background: #fff;
}

.tab_left {
  height: 100%;
  width:20%;
  text-align: center;
  font-size: 12px;
  background: #efefef;
}

.tab_lefts {
  width: auto;
  height: 45px;
  line-height: 45px;
  border-left: 6rpx solid transparent;
}

.tab_left>view {
  width: auto;
  height: 45px;
  line-height: 45px;
  overflow: hidden;
}

.tab_right {
  width:80%;
}
.vf{
    overflow: hidden;
}

.tab_right>view {
  width: 100%;
  height: 100%;
  background: red;
}

.type-item-on {
  border-left: 6rpx solid #e64340;
  color: #e64340;
  background: #fff;
}
.item{
   /* display: flex;
  flex-wrap: wrap;  */
  padding: 20px auto;
}
.item>view{
  width: 33%;
  height: 24%;
  float: left;
}
.item>view>image{
  width: 80%;
  margin: 0 auto;
  display: block
}
.item>view>view{
  text-align: center;
  font-size: 12px;
  line-height: 30px
}
.cricle{
  width: 150rpx;
  height: 150rpx;
  border-radius: 100%;
   border: 1px solid #ff0000; 
  background:url(http://sujiefs.com/upload/images/20170816/201708161803198792334_thumbnail.jpg)no-repeat;
  background-size: 100%;

}